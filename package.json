{
	"name": "nato-phonetic-alphabet",
	"version": "3.0.0",
	"description": "A tiny npm package that contains the full NATO phonetic alphabet (present \"International aviation\" variant), string converter and placeholder generator.",
	"keywords": [
		"Nato",
		"Phonetic",
		"Alphabet",
		"Converter",
		"List",
		"JSON",
		"Aviation",
		"Alphabet"
	],
	"author": "Jack van der Bilt <contact@managing.software> (https://managing.software)",
	"license": "MIT",
	"type": "module",
	"exports": {
		".": {
			"import": "./dist/index.js",
			"require": "./dist/index.cjs",
			"types": "./dist/index.d.ts"
		}
	},
	"files": [
		"dist"
	],
	"main": "dist/index.js",
	"module": "dist/index.js",
	"types": "dist/index.d.ts",
	"devDependencies": {
		"@swc/core": "^1.13.5",
		"@types/node": "^18.19.129",
		"@typescript-eslint/eslint-plugin": "^5.62.0",
		"@typescript-eslint/parser": "^5.62.0",
		"@vitest/coverage-v8": "^3.2.4",
		"tsup": "^6.7.0",
		"typescript": "^5.9.3",
		"vitest": "^3.2.4"
	},
	"scripts": {
		"dev": "tsup --watch src",
		"build": "tsup",
		"test": "vitest run",
		"test:coverage": "vitest run --coverage"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/UltraCakeBakery/nato-phonetic-alphabet.git"
	},
	"bugs": {
		"url": "https://github.com/UltraCakeBakery/nato-phonetic-alphabet/issues"
	},
	"homepage": "https://github.com/UltraCakeBakery/nato-phonetic-alphabet#readme",
	"funding": {
		"type": "patreon",
		"url": "https://www.patreon.com/jackvanderbilt"
	}
}
