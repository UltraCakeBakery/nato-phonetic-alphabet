{
	"name": "nato-phonetic-alphabet",
	"version": "2.1.0",
	"description": "A tiny npm package that contains the full NATO phonetic alphabet (present \"International aviation\" variant), string converter and placeholder generator.",
	"keywords": [
		"Converter",
		"Nato",
		"Phonetic",
		"Alphabet",
		"Alfa",
		"Alpha",
		"Bravo",
		"Beta",
		"List",
		"JSON",
		"Aviation",
		"Alphabet"
	],
	"author": "Jack van der Bilt <contact@managing.software> (https://managing.software)",
	"license": "MIT",
	"type": "module",
	"exports": {
		".": {
			"import": "./dist/index.js",
			"require": "./dist/index.cjs",
			"types": "./dist/index.d.ts"
		}
	},
	"files": [ "dist" ],
	"main": "dist/index.js",
	"module": "dist/index.js",
	"types": "dist/index.d.ts",
	"devDependencies": {
		"@swc/core": "^1.2.165",
		"@types/jest": "^28.1.0",
		"@types/node": "^18.0.0",
		"@typescript-eslint/eslint-plugin": "^5.18.0",
		"@typescript-eslint/parser": "^5.18.0",
		"eslint": "^8.13.0",
		"eslint-plugin-import": "^2.26.0",
		"eslint-plugin-jest": "^27.1.3",
		"jest": "^27.5.1",
		"ts-jest": "^27.1.4",
		"tsup": "^6.0.1",
		"typescript": "^4.6.3"
	},
	"scripts": {
		"dev": "tsup --watch src",
		"build": "tsup",
		"lint": "eslint .",
		"lint:fix": "eslint --fix --ext .js,.cjs,.ts .",
		"test": "jest --coverage --runInBand",
		"bench": "node ./benchmark/index.js"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/UltraCakeBakery/nato-phonetic-alphabet.git"
	},
	"bugs": {
		"url": "https://github.com/UltraCakeBakery/nato-phonetic-alphabet/issues"
	},
	"homepage": "https://github.com/UltraCakeBakery/nato-phonetic-alphabet#readme",
	"funding": {
		"type": "patreon",
		"url": "https://www.patreon.com/jackvanderbilt"
	}
}
